<h1>New line_of_business</h1>

<% form_for(@line_of_business) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :lob_code %><br />
    <%= f.text_field :lob_code %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :editable %><br />
    <%= f.text_field :editable %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', line_of_businesses_path %>