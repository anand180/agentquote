<h1>Listing group_permissions</h1>

<table>
  <tr>
    <th>Group code</th>
    <th>Activity code</th>
    <th>Lob code</th>
    <th>Status</th>
    <th>Editable</th>
  </tr>

<% @group_permissions.each do |group_permission| %>
  <tr>
    <td><%=h group_permission.group_code %></td>
    <td><%=h group_permission.activity_code %></td>
    <td><%=h group_permission.lob_code %></td>
    <td><%=h group_permission.status %></td>
    <td><%=h group_permission.editable %></td>
    <td><%= link_to 'Show', group_permission %></td>
    <td><%= link_to 'Edit', edit_group_permission_path(group_permission) %></td>
    <td><%= link_to 'Destroy', group_permission, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New group_permission', new_group_permission_path %>